      <div class="card-header">
        <h6 class="m-0 font-weight-bold title">Task</h6>
      </div>  
      <div class="card-body" >  
        <%= render partial: "manager/members/task", locals: { task_new: @task_new, task: @task,tasks: @tasks,member_id: @member_id }%>
      </div>
    </div>  
  </div>

    </div>
  </div>
