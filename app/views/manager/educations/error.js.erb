<% if params[:commit] == "教育項目追加" %>
$("#education-error").html("<%= j(render 'layouts/errors', model: @education_new) %>");
setTimeout(function(){$("#education-error").fadeOut('slow')}, 5000);
<% else %>
$("#education-error").html("<%= j(render 'layouts/errors', model: @education) %>");
setTimeout(function(){$("#education-error").fadeOut('slow')}, 5000);
<% end %>