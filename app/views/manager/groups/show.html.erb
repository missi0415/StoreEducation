<%= form_with model: @ability_title,url: manager_ability_titles_path,local:true do |f| %>
<%= f.text_field :title %>
<%= f.hidden_field :group_id,value: @group.id %>
<%= f.submit "保存する", class: "btn btn-success" %>
<% end %>
<% @ability_titles.each do |ability_title| %>
<%= ability_title.title %>
<% end %>
<h1>groups#show</h1>
<h2>mannagerグループを押した際に飛ぶ先</h2>
<h2><%= link_to "グループ連絡板", manager_group_message_path(@group) %></h2>
<h2><%= link_to "メンバー追加",new_member_registration_path(group_id: @group.id) %></h2>
<%= link_to "edit" ,edit_manager_group_path %>
<table class="table">
  <thead>
    <tr>
      <th>名前</th>
      <th>メンバー情報</th>
      <th>教育項目一覧</th>
      <th>課題一覧</th>
      <th>個人能力</th>
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
    <tr>
      <%= link_to manager_member_path(member) do %>
        <td><%= member.name %></td>
      <% end %>
    <td><%= link_to "情報" ,manager_member_path(member),class: "btn btn-success" %></td>
    <td><%= link_to "教育" ,manager_education_path(member),class: "btn btn-success" %></td>
    <td><%= link_to "課題" ,manager_task_path(member),class: "btn btn-success" %></td>
    <td><%= link_to "能力" ,manager_ability_path(member),data: {"turbolinks" => false},class: "btn btn-success" %></td>
    </tr>
    <% end %>
  </tbody>
</table>