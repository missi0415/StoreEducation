<h1>group#index</h1>
<p>ログインしてるmanagerのidは<%=current_manager.id %></p>

<%= form_with model:@group,local:true do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control goup_name' %>
  </div>
  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  </div>  
<% end %>
<% @groups.each do |group| %>
<p><%= group.name %></p>
<%= link_to edit_group_path ,class: "btn btn-success" %>
<%= link_to edit_group_path ,class: "btn danger" %>

<% end %>